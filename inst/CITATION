  note     = "R package version 0.1.0.9000",
  url      = "https://rapidsurveys.io/okapi/",
  textVersion = paste(
    "Ernest Guevarra (2023).",
    "okapi: Open Data Kit (ODK)-based Computer-assisted Personal Interview (CAPI) Tools",
    "R package version 0.1.0.9000",
    "URL https://rapidsurveys.io/okapi/"
  )
)
